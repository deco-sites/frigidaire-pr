{
  "__resolveType": "site/sections/Theme/Theme.tsx",
  "colorScheme": "any",
  "buttonStyle": {
    "--border-btn": "1px",
    "--rounded-btn": "0.2rem",
    "--btn-focus-scale": "1",
    "--animation-btn": "0.25s"
  },
  "otherStyles": {
    "--rounded-box": "1rem",
    "--rounded-badge": "1.9rem",
    "--animation-input": "0.2s",
    "--tab-border": "1px",
    "--tab-radius": "0.5rem"
  },
  "mode": "dark",
  "mainColors": {
    "base-100": "#FFFFFF",
    "primary": "#EE405A",
    "secondary": "#323333",
    "tertiary": "#ADADAD",
    "neutral": "#D6D6D6",
    "success": "#449D44",
    "warning": "#D6D6D6",
    "error": "#EF1C1C",
    "info": "#848585"
  },
  "complementaryColors": {
    "base-200": "#EBEBEB",
    "base-300": "#F5F5F5",
    "base-content": "#848585",
    "secondary-content": "#848585",
    "warning-content": "#848585",
    "error-content": "#FCD9DE"
  },
  "font": {
    "__resolveType": "website/loaders/fonts/googleFonts.ts",
    "fonts": [
      {
        "variations": [
          {
            "weight": "200"
          },
          {
            "weight": "300"
          },
          {
            "weight": "400"
          },
          {
            "weight": "500"
          },
          {
            "weight": "600"
          },
          {
            "weight": "700"
          }
        ],
        "family": "Noto Sans"
      }
    ]
  }
}